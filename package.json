{
  "name": "prettier-eslint",
  "version": "0.0.0-development",
  "description": "Formats your JavaScript using prettier followed by eslint --fix",
  "main": "dist/index.js",
  "scripts": {
    "start": "nps",
    "test": "nps test",
    "commitmsg": "opt --in commit-msg --exec \"validate-commit-msg\"",
    "precommit": "opt --in pre-commit --exec \"npm start validate\"",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "Kent C. Dodds <kent@doddsfamily.us> (http://kentcdodds.com/)",
  "license": "MIT",
  "dependencies": {
    "eslint": "^3.13.1",
    "prettier": "^0.0.5"
  },
  "devDependencies": {
    "all-contributors-cli": "^3.0.7",
    "babel-cli": "^6.18.0",
    "babel-jest": "18.0.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "babel-register": "^6.18.0",
    "codecov": "^1.0.1",
    "commitizen": "^2.8.6",
    "cz-conventional-changelog": "^1.2.0",
    "eslint-config-kentcdodds": "^11.1.0",
    "husky": "0.12.0",
    "jest-cli": "18.1.0",
    "opt-cli": "^1.5.1",
    "p-s": "^3.0.3",
    "rimraf": "^2.5.4",
    "semantic-release": "^6.3.2",
    "strip-indent": "^2.0.0",
    "validate-commit-msg": "^2.8.2"
  },
  "eslintConfig": {
    "extends": [
      "kentcdodds",
      "kentcdodds/jest"
    ],
    "rules": {
      "valid-jsdoc": 0
    }
  },
  "jest": {
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kentcdodds/prettier-eslint.git"
  },
  "bugs": {
    "url": "https://github.com/kentcdodds/prettier-eslint/issues"
  },
  "homepage": "https://github.com/kentcdodds/prettier-eslint#readme"
}
